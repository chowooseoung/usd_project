#usda 1.0
(
    defaultPrim = "payload"
)

over "payload" (
    variants = {
        string geom = "sphere"
    }
    append variantSets = "geom"
)
{
    variantSet "geom" = {
        "egg" (
            prepend references = @./vset_geom/ball.egg.P.usda@
        ) {

        }
        "sphere" (
            prepend references = @./vset_geom/ball.sphere.P.usda@
        ) {

        }
    }
}


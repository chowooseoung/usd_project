#usda 1.0
(
    defaultPrim = "payload"
)

over "payload" (
    variants = {
        string geom = "sphere"
    }
    append variantSets = "geom"
)
{
    variantSet "geom" = {
        "egg" (
            prepend references = @./egg/ball.egg.version.P.usda@
        ) {

        }
        "sphere" (
            prepend references = @./sphere/ball.sphere.version.P.usda@
        ) {

        }
    }
}


#usda 1.0
(
    defaultPrim = "ball"
)

def Xform "ball" (
    prepend payload = [
        @./vset/vset.usda@,
        @./model_payload.usda@
    ]
    variants = {
        string instancing = "on"
    }
    append variantSets = "instancing"
)
{
    variantSet "instancing" = {
        "off" {
            over "payload" (
                instanceable = false
            )
            {
            }

        }
        "on" {
            over "payload" (
                instanceable = true
            )
            {
            }

        }
    }
}

